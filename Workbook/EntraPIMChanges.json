              "version": "KqlItem/1.0",
              "query": "AuditLogs\r\n| where Category == \"RoleManagement\"\r\n| where OperationName == \"Update role setting in PIM\"\r\n| summarize count() by OperationName, bin(TimeGenerated, 1d)",
