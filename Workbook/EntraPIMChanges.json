    {
      "type": 3,
      "content": {
        "version": "KqlItem/1.0",
        "query": "AuditLogs\r\n| where CorrelationId == \"{SelectedCorrelationId}\"\r\n| where Category == \"RoleManagement\" or Category == \"GroupManagement\"\r\n| where OperationName == \"Update role setting in PIM\"\r\n| mv-apply item = AdditionalDetails on (\r\n    where tostring(item.key) == \"ipaddr\"\r\n    | extend ipaddr = tostring(item.value)\r\n    )\r\n| mv-apply item = AdditionalDetails on (\r\n    where tostring(item.key) == \"UserAgent\"\r\n    | extend UserAgent = tostring(item.value)\r\n    )\r\n| extend geo_ip = tostring(geo_info_from_ip_address(ipaddr))\r\n| sort by TimeGenerated asc \r\n| project\r\n    TimeGenerated,\r\n    ipaddr,\r\n    geo_ip,\r\n    CorrelationId\r\n    | project geo_ip, ipaddr\r\n    | extend IPAddress = ipaddr\r\n    | extend longitude = tostring(parse_json(geo_ip).longitude)\r\n    | extend latitude = tostring(parse_json(geo_ip).latitude)\r\n    | extend Country = tostring(parse_json(geo_ip).country)\r\n    | extend State = tostring(parse_json(geo_ip).state)\r\n    | extend City = tostring(parse_json(geo_ip).city)\r\n    | project IPAddress, Country, State,City, longitude, latitude\r\n    \r\n\r\n    \r\n    ",
        "size": 1,
        "title": "Location of Change",
        "timeContextFromParameter": "TimeRange",
        "queryType": 0,
        "resourceType": "microsoft.operationalinsights/workspaces",
        "crossComponentResources": [
