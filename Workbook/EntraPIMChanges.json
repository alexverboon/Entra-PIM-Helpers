        "version": "NotebookGroup/1.0",
        "groupType": "editable",
        "title": "PIM Changes Overview",
        "items": [
          {
            "type": 3,
            "content": {
              "version": "KqlItem/1.0",
              "query": "AuditLogs\r\n| where Category == \"RoleManagement\"\r\n| where OperationName == \"Update role setting in PIM\"\r\n| project modifiedBy=tostring(InitiatedBy.user.userPrincipalName)\r\n| summarize Changes = count() by modifiedBy\r\n| order by Changes desc",
              "size": 1,
              "title": "Privileged Roles & Groups Top editors",
              "timeContextFromParameter": "TimeRange",
              "queryType": 0,
              "resourceType": "microsoft.operationalinsights/workspaces",
              "crossComponentResources": [
                "{Workspace}"
              ],
              "gridSettings": {
                "rowLimit": 10
              }
            },
            "customWidth": "60",
            "name": "Top Editors",
            "styleSettings": {
              "showBorder": true
